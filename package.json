{
  "name": "@sablier/safe-app",
  "description": "Safe App for interacting with the Sablier protocol",
  "version": "1.0.0",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "contributors": [
    {
      "name": "Paul Razvan Berg",
      "url": "https://github.com/paulrberg"
    },
    {
      "name": "Tom French",
      "url": "https://github.com/tomafrench"
    }
  ],
  "dependencies": {
    "@gnosis.pm/safe-apps-sdk": "^0.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "6.x",
    "eslint-config-airbnb-typescript-prettier": "^2.1.1",
    "eslint-plugin-react-hooks": "2.x",
    "husky": "^4.2.5",
    "prettier": "1.x",
    "react-scripts": "3.4.1",
    "shx": "^0.3.2",
    "typescript": "3.7.3"
  },
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "clean": "shx rm -rf ./build",
    "eject": "react-scripts eject",
    "lint": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore src/**/*.{js,jsx,ts,tsx}",
    "prettier": "prettier --config .prettierrc --write '**/*.{js,json,jsx,md,ts,tsx}'",
    "prettier:list-different": "prettier --config .prettierrc --list-different '**/*.{js,json,jsx,md,ts,tsx}'",
    "start": "react-scripts start"
  }
}
